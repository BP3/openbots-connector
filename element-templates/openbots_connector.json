{
  "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name": "OpenBots Connector",
  "id": "6a341cd2-8d96-4d03-bb3b-371b5b15fbcd",
  "version": 1704893310752,
  "description": "Orchestrate your OpenBots with Camunda. You can create new queue items and get the status from it",
  "icon": {
    "contents": "data:image/png;base64,"
  },
  "category": {
    "id": "connectors",
    "name": "Connectors"
  },
  "appliesTo": [
    "bpmn:Task"
  ],
  "elementType": {
    "value": "bpmn:ServiceTask"
  },
  "groups": [
    {
      "id": "operation",
      "label": "Operation"
    },
    {
      "id": "authentication",
      "label": "Authentication"
    },
    {
      "id": "configuration",
      "label": "Configuration"
    },
    {
      "id": "input",
      "label": "Input"
    },
    {
      "id": "output",
      "label": "Output"
    },
    {
      "id": "errors",
      "label": "Error handling"
    }
  ],
  "properties": [
    {
      "type": "Hidden",
      "value": "io.camunda:http-json:1",
      "binding": {
        "type": "zeebe:taskDefinition:type"
      }
    },
    {
      "label": "Bearer token",
      "group": "authentication",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "token"
      },
      "constraints": {
        "notEmpty": true
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "addQueueItem",
          "getItemById"
        ]
      }
    },
    {
      "label": "OpenBots Operation type",
      "description": "The operation in OpenBots to invoke",
      "id": "operationType",
      "group": "operation",
      "type": "Dropdown",
      "choices": [
        {
          "name": "Login",
          "value": "login"
        },
        {
          "name": "Add queue item",
          "value": "addQueueItem"
        },
        {
          "name": "Get queue item result by ID",
          "value": "getItemById"
        }
      ],
      "binding": {
        "type": "zeebe:input",
        "name": "operationType"
      }
    },
    {
      "id": "methodPost",
      "group": "configuration",
      "type": "Hidden",
      "value": "post",
      "binding": {
        "type": "zeebe:input",
        "name": "method"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "addQueueItem",
          "login"
        ]
      }
    },
    {
      "id": "methodGet",
      "group": "configuration",
      "type": "Hidden",
      "value": "get",
      "binding": {
        "type": "zeebe:input",
        "name": "method"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "getItemById"
        ]
      }
    },
    {
      "label": "OpenBots instance URL",
      "group": "configuration",
      "type": "String",
      "binding": {
        "type": "zeebe:input",
        "name": "cloudUrl"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Organization ID",
      "group": "configuration",
      "description": "OpenBots organization ID",
      "type": "String",
      "feel": "optional",
      "optional": false,
      "binding": {
        "type": "zeebe:input",
        "name": "organizationId"
      },
      "constraints": {
        "notEmpty": true
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "addQueueItem",
          "getItemById"
        ]
      }
    },
    {
      "label": "Queue ID",
      "description": "The ID of the queue to which to create the queue item",
      "group": "configuration",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "queueId"
      },
      "constraints": {
        "notEmpty": true
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "addQueueItem"
        ]
      }
    },
    {
      "label": "Username",
      "description": "Username",
      "group": "authentication",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "loginUsername"
      },
      "constraints": {
        "notEmpty": false
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "login"
        ]
      }
    },
    {
      "label": "Password",
      "description": "Password",
      "group": "authentication",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "loginPassword"
      },
      "constraints": {
        "notEmpty": false
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "login"
        ]
      }
    },
    {
      "group": "configuration",
      "type": "Hidden",
      "value": "={\"Accept\": \"application/json\",\"Content-Type\": \"application/json\",\"organizationid\":organizationId, \"Authorization\": \"Bearer \"+token}",
      "binding": {
        "type": "zeebe:input",
        "name": "headers"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Queue Item name",
      "description": "The name of the queue item that you are creating",
      "group": "input",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "queueItemName"
      },
      "constraints": {
        "notEmpty": true
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "addQueueItem"
        ]
      }
    },
    {
      "label": "Queue information for the OpenBots job",
      "description": "dataJson: This is the json data that is required as part of the new queue item that is to be created. It should be in the JSON format",
      "group": "input",
      "type": "Text",
      "feel": "required",
      "binding": {
        "type": "zeebe:input",
        "name": "queueItemBody"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "addQueueItem"
        ]
      },
      "optional": true
    },
    {
      "group": "input",
      "type": "Hidden",
      "value": "={ \"name\": queueItemName, \"queueId\": queueId }",
      "binding": {
        "type": "zeebe:input",
        "name": "minimalQueueItemBody"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "addQueueItem"
        ]
      },
      "optional": true
    },
    {
      "label": "Item ID",
      "description": "The item ID to be fetched",
      "group": "input",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "itemId"
      },
      "constraints": {
        "notEmpty": true
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "getItemById"
        ]
      }
    },
    {
      "group": "input",
      "type": "Hidden",
      "value": "= if queueItemBody != null then context merge(queueItemBody,{ \"queueId\": queueId, \"name\": if string length(queueItemName) > 0 then queueItemName else queueItemBody.name }) else minimalQueueItemBody ",
      "binding": {
        "type": "zeebe:input",
        "name": "body"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "addQueueItem"
        ]
      },
      "optional": true
    },
    {
      "group": "input",
      "type": "Hidden",
      "value": "= { \"userName\": loginUsername, \"password\": loginPassword } ",
      "binding": {
        "type": "zeebe:input",
        "name": "body"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "login"
        ]
      },
      "optional": true
    },
    {
      "label": "Result variable",
      "description": "Name of variable to store the response in. Details in the <a href=\"https://docs.camunda.io/docs/components/connectors/use-connectors/#result-variable\" target=\"_blank\">documentation</a>",
      "group": "output",
      "type": "String",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultVariable"
      }
    },
    {
      "label": "Result expression",
      "id": "resultExpressionGet",
      "description": "Expression to map the response into process variables. Details in the <a href=\"https://docs.camunda.io/docs/components/connectors/use-connectors/#result-expression\" target=\"_blank\">documentation</a>",
      "group": "output",
      "type": "Text",
      "feel": "required",
      "value": "",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultExpression"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "addQueueItem"
        ]
      }
    },
    {
      "label": "Result expression",
      "id": "resultExpressionGet",
      "description": "Expression to map the response into process variables. Details in the <a href=\"https://docs.camunda.io/docs/components/connectors/use-connectors/#result-expression\" target=\"_blank\">documentation</a>",
      "group": "output",
      "type": "Text",
      "feel": "required",
      "value": "={itemStatus: response.body.state}",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultExpression"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "getItemById"
        ]
      }
    },
    {
      "label": "Result expression",
      "id": "resultExpressionGet",
      "description": "Expression to map the response into process variables. Details in the <a href=\"https://docs.camunda.io/docs/components/connectors/use-connectors/#result-expression\" target=\"_blank\">documentation</a>",
      "group": "output",
      "type": "Text",
      "feel": "required",
      "value": "={bearerToken: response.body.accessToken}",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultExpression"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "login"
        ]
      }
    },
    {
      "label": "Connection timeout",
      "description": "Sets the timeout in seconds to establish a connection or 0 for an infinite timeout",
      "group": "errors",
      "type": "String",
      "value": "20",
      "binding": {
        "type": "zeebe:input",
        "name": "connectionTimeoutInSeconds"
      },
      "optional": true,
      "constraints": {
        "notEmpty": false,
        "pattern": {
          "value": "^(=.+|[0-9]+|\\{\\{secrets\\..+\\}\\})$",
          "message": "Must be a timeout in seconds (default value is 20 seconds) or a FEEL expression"
        }
      }
    },
    {
      "type": "Hidden",
      "value": "=\"https:\"+\"//\"+cloudUrl+\"/queueitems/enqueue\"",
      "binding": {
        "type": "zeebe:input",
        "name": "url"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "addQueueItem"
        ]
      }
    },
    {
      "group": "configuration",
      "type": "Hidden",
      "value": "=\"https:\"+\"//\"+cloudUrl+\"/queueitems/\"+itemId",
      "binding": {
        "type": "zeebe:input",
        "name": "url"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "getItemById"
        ]
      }
    },
    {
      "group": "configuration",
      "type": "Hidden",
      "value": "=\"https:\"+\"//\"+cloudUrl+\"/auth/machine/token/\"",
      "binding": {
        "type": "zeebe:input",
        "name": "url"
      },
      "condition": {
        "property": "operationType",
        "oneOf": [
          "login"
        ]
      }
    },
    {
      "label": "Error expression",
      "description": "Expression to handle errors. Details in the <a href=\"https://docs.camunda.io/docs/components/connectors/use-connectors/#bpmn-errors\" target=\"_blank\">documentation</a>",
      "group": "errors",
      "type": "Text",
      "feel": "required",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "errorExpression"
      }
    }
  ]
}